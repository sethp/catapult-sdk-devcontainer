{
  "name": "Catapult SDK",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": "true",
      "username": "vscode",
      "userUid": "1000",
      "userGid": "1000"
    },
    "ghcr.io/devcontainers/features/git:1": {
      "version": "latest",
      "ppa": "false"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ImaginationTech.catapult-studio-pack",
        "llvm-vs-code-extensions.vscode-clangd"
      ]
    }
  },
  "remoteUser": "vscode"
}
